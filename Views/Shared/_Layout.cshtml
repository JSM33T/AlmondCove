@using almondCove.Models.Props;

@{
    var htmlProps = ViewData["HtmlProps"] as HTMLProps;
    var metaProps = ViewData["MetaProps"] as MetaProps;
}


<!DOCTYPE html>
<html lang="en" data-bs-theme="light">
